{
  "steps": [
    {
      "name": "node:18",
      "entrypoint": "npm",
      "args": ["install"],
      "id": "install-dependencies"
    },
    {
      "name": "node:18",
      "entrypoint": "npm",
      "args": ["run", "build"],
      "id": "build",
      "waitFor": ["install-dependencies"],
      "env": ["NODE_ENV=production"]
    }
  ],
  "timeout": "1200s",
  "options": {
    "logging": "CLOUD_LOGGING_ONLY"
  },
  "images": []
}


